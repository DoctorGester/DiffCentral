#GroupList .ListSegmentDisplayed1,
#GroupList .ListSegmentHiddenBefore1,
#GroupList .ListSegmentHiddenAfter1
{
	x: 0px;
	y: 0px;
}

#GroupList .ListSegmentHiddenBefore1,
#GroupList .ListSegmentHiddenBefore2,
#GroupList .ListSegmentHiddenBefore3,
#GroupList .ListSegmentHiddenBefore4
{
	x: -500px;
}

#GroupList .ListSegmentHiddenAfter1,
#GroupList .ListSegmentHiddenAfter2,
#GroupList .ListSegmentHiddenAfter3,
#GroupList .ListSegmentHiddenAfter4
{
	x: 500px;
}

#GroupGrid .ListSegmentDisplayed1,
#GroupGrid .ListSegmentHiddenBefore1,
#GroupGrid .ListSegmentHiddenAfter1
{
	x: 0px;
	y: 0px;
}

#GroupGrid .ListSegmentHiddenBefore1,
#GroupGrid .ListSegmentHiddenBefore2,
#GroupGrid .ListSegmentHiddenBefore3,
#GroupGrid .ListSegmentHiddenBefore4
{
	x: 100%;
}

#GroupGrid .ListSegmentHiddenAfter1,
#GroupGrid .ListSegmentHiddenAfter2,
#GroupGrid .ListSegmentHiddenAfter3,
#GroupGrid .ListSegmentHiddenAfter4
{
	x: -100%;
}

