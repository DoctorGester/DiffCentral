	color: white;
	text-shadow: 0px 0px 20px 1.5 #3382ff;
}

#ZooButton
{
	margin-top: 12px;
	visibility: collapse;
}

.MainBranch #ZooButton
{
	visibility: visible;
}

#ZooButton Label
{
	margin-top: 2px;
	vertical-align: middle;
	text-align: center;
}


.DebugButton
{
	width: 32px;
	height: 32px;
	margin-top: 14px;
	background-size: 20px 20px;
	background-repeat: no-repeat;
	background-position: 50% 50%;
	wash-color: #66aadd;
}

.DebugButton:hover
{
	wash-color: #dcdcdc;
}

.DebugButton:active
{
	wash-color: white;
}

#BugButton
{
	margin-right: 5px;
	margin-left: 5px;
	transform: rotateZ( 20deg ) ;
	background-image: url("s2r://panorama/images/topbar/bug_png.vtex");
	visibility: collapse;
}

.MainBranch #BugButton
{
	visibility: visible;
}

#GuidesButton
{
	background-image: url("s2r://panorama/images/topbar/guides_png.vtex");
	margin-right: 30px;
	visibility: collapse;
}

.MainBranch #GuidesButton
{
	visibility: visible;
}

#ReloadPageButton
{
	background-image: url("s2r://panorama/images/control_icons/refresh_psd.vtex");
	visibility: collapse;
}

.MainBranch #ReloadPageButton
{
	visibility: visible;
}

#TopBarLeftButtons
{
	flow-children: right;
}

#TopBarRightButtons
{
	horizontal-align: right;
	flow-children: right;
}

#TestBuildLabel
{
	color: red;
	font-weight: bold;
	vertical-align: middle;
	font-size: 15px;
	visibility: collapse;
	text-transform: uppercase;
	margin-right: 2px;
}

.StagingBranch #TestBuildLabel
{
	visibility: visible;
}

#NavigationControls
{
	margin-left: 10px;
	flow-children: right;
	vertical-align: middle;
}

#NavigateBackButton
{
	background-image: url("s2r://panorama/images/control_icons/arrow_solid_left_png.vtex");
}
#NavigateForwardButton
{
	background-image: url("s2r://panorama/images/control_icons/arrow_solid_right_png.vtex");
}

#NavigationControls Button
{
	margin: 0px;
	width: 24px;
	height: 57px;
	background-size: 18px 28px;
	background-repeat: no-repeat;
	background-position: 50% 50%;
	wash-color: #444a55;
	
	transition-property: background-color, wash-color;
	transition-delay: 0.0s;
	transition-duration: 0.2s;
	transition-timing-function: ease-out;
}

#NavigationControls Button:enabled:hover
{
	wash-color: #9999aa;
}

#NavigationControls Button:active
{
	wash-color: #ddddff;
}

#NavigationControls Button:disabled
{
	wash-color: #111111;
}

#BackgroundImage, #BackgroundVignette
{
	width: 100%;
	height: 100%;
	background-repeat: no-repeat;
	background-size: 100% 100%;
}

#BackgroundImage
{
	background-image: url("s2r://panorama/images/backgrounds/dashboard_background_psd.vtex");
	background-color: black; /* fallback for when the image isn't loaded yet */
	opacity-mask: url("s2r://panorama/images/masks/db_isingame_mask_psd.vtex") 0.0;
}

DOTADashboard.IsInGame #BackgroundImage
{
	opacity-mask: url("s2r://panorama/images/masks/db_isingame_mask_psd.vtex") 1.0;
}

#BackgroundVignette { background-image: url("s2r://panorama/images/backgrounds/dashboard_vignette_png.vtex"); }

DOTADashboard.PlayTabVisible #DashboardBackground, DOTADashboard.PopupBackgroundBlur #DashboardBackground
{
	//blur: gaussian( 5, 2, 2 );
	wash-color: #000000aa;
}


DOTADashboard.PlayTabVisible #DashboardBackground
{
	transform: translatex( -150px );    
}


#DashboardPages, #DashboardBackground
{
	width: 100%;
	height: 100%;
	wash-color: white;

	transform: none;
	transition-property: transform, blur, saturation, wash-color;
	transition-duration: 0.45s;
	transition-delay: 0.0s;
	transition-timing-function: ease-in;
}

#DashboardBackgroundStatus
{
	width: 50%;
	height: 50%;
	align: right bottom;
}

#DashboardTopBarBackgroundGradient
{
	width: 100%;
	height: 150px;
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #000000 ), to( #00000000 ) );
}

#DashboardBackgroundInitContainer
{
	width: 100%;
	height: 100%;
	wash-color: white;

	transform: none;
	transition-property: transform, blur, saturation, wash-color;
	transition-duration: 2s;
	transition-delay: 0.0s;
	transition-timing-function: ease-in-out;
}

DOTADashboard.InitialDisplay #DashboardBackgroundInitContainer
{
	wash-color: black;
	transition-delay: 0.65s;
}

DOTADashboard.PlayTabVisible #DashboardPages, DOTADashboard.PlayTabVisible #TopBarSecondaryContainer, DOTADashboard.PopupBackgroundBlur #DashboardForeground, DOTADashboard.PopupBackgroundBlur #Chat
{
	blur: gaussian( 5, 2, 2 );
	wash-color: #657b7baa;
	saturation: 0.25;
}

DOTADashboard.PlayTabVisible #DashboardPages, DOTADashboard.PlayTabVisible #Chat
{
	transform: translatex( -120px );    
	//transform: translatex( 0px );    
}

DOTADashboard.AspectRatio16x10.PlayTabVisible #DashboardPages, DOTADashboard.AspectRatio16x10.PlayTabVisible #Chat
{
	transform: translatex( -208px );    
}

DOTADashboard.AspectRatio16x10.NotOnTodayPage.PlayTabVisible #Chat
{
	transform: translatex( 0px ) translateY( 109px );    
}

DOTADashboard.AspectRatio4x3.NotOnTodayPage.PlayTabVisible #Chat
{
	transform: translateX( 0px ) translateY( 109px );
}

DOTADashboard.AspectRatio4x3.DBLobbyVisible.PlayTabVisible #Chat
{
	transform: translateX( -350px ) translateY( 0px );
}

	
DOTADashboard.PlayTabVisible #DashboardPagesBlocker
{
//	background-color: #4B5B5B05;
}

#DashboardPagesGuides
{
	width: 100%;
	height: 100%;
	visibility: collapse;
	horizontal-align: center;
	vertical-align: middle;
}

#DashboardPagesGuides.Visible
{
	visibility: visible;
}

#DashboardPagesGuides Panel
{
	vertical-align: middle;
	horizontal-align: center;
	border: 1px solid #66aadd33;
	height: 100%;
	margin: 150px 10px 130px 10px;
}

#Guide4x3
{
	width: height-percentage( 174% );
}

/* toggled hittest on when the play tab is open */
#DashboardPagesBlocker
{
	width: 100%;
	height: 100%;
	
	transition-property: background-color;
	transition-duration: 0.3s;
	transition-timing-function: ease-in-out;
}

DOTADashboard.PlayTabVisible #DashboardPagesBlocker:hover
{
	//background-color: #4B5B5B11;
}

#TopBarSecondaryContainer
{
	width: 100%;
	padding-bottom: 12px;

	transition-property: blur, transform;
	transition-duration: 0.30s;
	transition-timing-function: ease-in-out;
}

#TopBarSecondaryContainer.ProfileVisible
{
	margin-top: 70px;
	width: 1410px;
	horizontal-align: center;

	// match transition time of DashboardPages
	transition-duration: 0.45s;
}

#TopBarSecondaryContent
{
	flow-children: right;
	width: 100%;
	height: 51px;
	margin-top: 59px;
	opacity: 1.0;
	transform: translatey( 0px );

	transition-property: transform, opacity;
	transition-duration: 0.30s;
	transition-timing-function: ease-in-out;
}

DOTADashboard.GCStatusVisible #TopBarSecondaryContainer
{
	transform: translateY( 30px );
	transition-duration: 0.2s; 
}

DOTADashboard.GCStatusVisible #TopBarSecondaryContainer.ProfileVisible
{
	transform: translateY( 0px );
}

#TopBarSecondaryContent.Hidden
{
	transform: translatey( -45% );
	opacity: 1.0;
}

#TopBarSecondaryContent.Hidden .Spinner
{
	opacity: 0.0;
}

#TopBarSecondaryContent.Hidden #TopBarSecondaryTabs
{
	transform: translatey( -65% );
	opacity: 0.0;
}

#TopBarSecondaryContent.Hidden .SecondaryTabButton
{
	wash-color: black;
}

#TopBarSecondaryTabs
{
	width: fill-parent-flow( 1.0 );
	background-color: gradient( linear, 0% 0%, 100% 0%, from( #000000aa ), color-stop( 0.8, #000000aa ),  to( #00000000 ) );
	height: 100%;
	flow-children: right;
	vertical-align: middle;
	
	padding-left: 437px;
	padding-top: 14px;
	padding-bottom: 7px;
	
	opacity: 1.0;
	
	transition-property: transform, opacity;
	transition-duration: 0.3s;
	transition-timing-function: ease-in-out;
}

.AspectRatio5x4 #TopBarSecondaryTabs
{
	padding-left: 400px;
}

#TopBarSecondaryContainer.ProfileVisible #TopBarSecondaryTabs
{
	padding-left: 16px;
	background-color: #00000000;
}


.SecondaryTabButton
{
	margin: 0px;
	vertical-align: middle;
	wash-color: white;	
	
	transition-property: wash-color;
	transition-duration: 0.6s;
	transition-timing-function: ease-in-out;
}

.SecondaryTabButton Label
{
	color: #768e8d;
	vertical-align: middle;
	font-size: 20px;
	text-transform: uppercase;
	margin: 0px;
	letter-spacing: 3px;
}

.SecondaryTabButton:disabled Label
{
	color: #768e8d33;
}

.Language_schinese .SecondaryTabButton Label
{
	font-size: 20px;
	letter-spacing: 4px;
}


.AspectRatio4x3 .SecondaryTabButton Label,
.AspectRatio5x4 .SecondaryTabButton Label
{
	letter-spacing: 0px;
