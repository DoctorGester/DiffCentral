<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_match_details.vcss_c" />
	</styles>

	<Panel class="PlayerRowLegend">
		<Label class="LegendPlayer" text="#dota_page_match_details_legend_player" />
		<Label class="LegendLevel" text="#dota_page_match_details_legend_level" />
		<Label class="LegendHero" text="#dota_page_match_details_legend_hero" />
		<Label class="LegendKills" text="#dota_page_match_details_legend_kills" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_kills_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendDeaths" text="#dota_page_match_details_legend_deaths" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_deaths_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendAssists" text="#dota_page_match_details_legend_assists" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_assists_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendItems" text="#dota_page_match_details_legend_items" />
		<Label class="LegendGold" text="#dota_page_match_details_legend_gold" />
		<Label class="LegendLastHits" text="#dota_page_match_details_legend_last_hits" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_last_hits_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendDenies" text="#dota_page_match_details_legend_denies" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_denies_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendGoldPerMin" text="#dota_page_match_details_legend_gold_per_min" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_gold_per_min_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendXPPerMin" text="#dota_page_match_details_legend_xp_per_min" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_xp_per_min_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
	</Panel>
</root>
