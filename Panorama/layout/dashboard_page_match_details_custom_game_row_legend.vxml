<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_match_details.vcss_c" />
	</styles>

	<Panel class="CustomGamePlayerRowLegend">
		<Image class="LegendWinner" src="s2r://panorama/images/control_icons/winner_psd.vtex" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_winner_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendPlayer" text="#dota_page_match_details_legend_player" />
		<Label class="LegendLevel" text="#dota_page_match_details_legend_level" />
		<Label class="LegendHero" text="#dota_page_match_details_legend_hero" />
		<Label class="LegendKills" text="#dota_page_match_details_legend_kills" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_kills_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendDeaths" text="#dota_page_match_details_legend_deaths" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_deaths_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
		<Label class="LegendAssists" text="#dota_page_match_details_legend_assists" onmouseover="DOTAShowTextTooltipStyled( #dota_page_match_details_legend_assists_tooltip, ShortTextTooltip )" onmouseout="DOTAHideTextTooltip()" />
	</Panel>
</root>
