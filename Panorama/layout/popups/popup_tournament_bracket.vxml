<root>
  <styles>
    <include src="s2r://panorama/styles/dotastyles.vcss_c" />
    <include src="s2r://panorama/styles/dashboard.vcss_c" />
    <include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
    <include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
    <include src="s2r://panorama/styles/popups/popup_tournament_bracket.vcss_c" />
  </styles>

  <PopupTournamentBracket class="PopupPanel Hidden BigPopup" popupbackground="dim" oncancel="DOTAPopupButtonClicked()">
    <Panel id="TournamentMainPanel">
      <Panel id="TournamentHeader">
        <Image id="TournamentLogo" src="s2r://panorama/images/items/boots_png.vtex"/>
        <Panel id="TournamentInfo">
          <Label id="TournamentName" text="Dota 2 Asia Championships"/>
          <Panel id="TournamentMenu">
              <RadioButton class="SecondaryTabButton Activated" onactivate="DOTATournamentBracketSelectTab( 1 )" selected="true" >
                <Panel class="RadioBox"/>
                <Label text="Overview" />
              </RadioButton>
              <Label class="SecondaryTabSeparator" text="/" />
              
            </Panel>

          </Panel>
        </Panel>
      <Panel id="TabPanels">
        
      </Panel>
      <Panel id="InfoRow">
        <Panel id="TournamentInfo">
          <Panel class="TournamentTextInfo" id="TournamentPrizePool">
            <Label class="TournamentTextLabel" text="Current Prize Pool: "/>
            <Label text="$3,000,000"/>
          </Panel>
          <Panel class="TournamentTextInfo" id="TournamentDates">
            <Label class="TournamentTextLabel" text="Dates: "/>
            <Label text="Feb 5-9, 2015"/>
          </Panel>
          <Panel class="TournamentTextInfo" id="TournamentLocation">
            <Label class="TournamentTextLabel" text="Location: "/>
            <Label text="Feb 5-9, 2015"/>
          </Panel>
        </Panel>
      </Panel>
    </Panel>
    <Panel id="TournamentSidePanel">
      <Panel id="SpoilersPanel">
        <Button id="SpoilerButton" class="PopupButton" onactivate="DOTAAddFriendSearchClicked()">
          <Label text="Spoiler check" />     
        </Button>
        <Image id="SpoilerImage" src="s2r://panorama/images/items/boots_png.vtex"/>
      </Panel>
      
      <Panel id="UpcomingMatchesPanel">
        <Label id="UpcomingMatchesLabel" text="Upcoming Matches"/>
        <Panel id="UpcomingMatch1" class="UpcomingMatch">
          <Panel class="UpcomingMatchVersusPanel" id="UpcomingMatch1VersusPanel">
            <Panel id="UpcomingMatch1Team1">
              <Image id="UpcomingMatch1Team1Img" class="UpcomingMatchTeam1Img" src="s2r://panorama/images/items/boots_png.vtex"/>
            </Panel>
            <Label class="UpcomingMatchesLabel" id="UpcomingMatchesLabel1" text="1 : 0"/>
            <Panel id="UpcomingMatch1Team2" class="UpcomingMatchTeam2">
              <Image id="UpcomingMatch2Team2Img" class="UpcomingMatchTeam2Img" src="s2r://panorama/images/items/boots_png.vtex"/>
            </Panel>
          </Panel>
          <Label id="UpcomingMatch1Stage" class="UpcomingMatchStage" text="Lower Bracket Round 4 (Bo3)"/>
          <Label id="UpcomingMatch1Status" class="UpcomingMatchStatus" text="In progress - Watch Live"/>
          <Label id="UpcomingMatch1WinnerStakes" class="UpcomingMatchWinnerStakes" text="Winner advances to Lower Finals"/>
          <Label id="UpcomingMatch1LoserStakes" class="UpcomingMatchLoserStakes" text="Loser is eliminated, Wins $125,523"/>
        </Panel>
        <Panel id="UpcomingMatch2" class="UpcomingMatch">
          <Panel class="UpcomingMatchVersusPanel" id="UpcomingMatch2VersusPanel">
            <Panel id="UpcomingMatch2Team1">
              <Image id="UpcomingMatch2Team1Img" class="UpcomingMatchTeam1Img" src="s2r://panorama/images/items/boots_png.vtex"/>
            </Panel>
            <Label class="UpcomingMatchesLabel" id="UpcomingMatchesLabel2" text="VS"/>
            <Panel id="UpcomingMatch2Team2" class="UpcomingMatchTeam2">
              <Image id="UpcomingMatch2Team2Img" class="UpcomingMatchTeam2Img" src="s2r://panorama/images/items/boots_png.vtex"/>
            </Panel>
          </Panel>
          <Label id="UpcomingMatch2Stage" class="UpcomingMatchStage" text="Lower Bracket Round 4 (Bo3)"/>
          <Label id="UpcomingMatch2Status" class="UpcomingMatchStatus" text="Starting in 02:00"/>
          <Label id="UpcomingMatch2WinnerStakes" class="UpcomingMatchWinnerStakes" text="Winner advances to Lower Finals"/>
          <Label id="UpcomingMatch2LoserStakes" class="UpcomingMatchLoserStakes" text="Loser is eliminated, Wins $125,523"/>
        </Panel>
        <Panel id="UpcomingMatch3" class="UpcomingMatch">
          <Panel class="UpcomingMatchVersusPanel" id="UpcomingMatch3VersusPanel">
            <Panel id="UpcomingMatch3Team1">
              <Image id="UpcomingMatch3Team1Img" class="UpcomingMatchTeam1Img" src="s2r://panorama/images/items/boots_png.vtex"/>
            </Panel>
            <Label class="UpcomingMatchesLabel" id="UpcomingMatchesLabel3" text="VS"/>
            <Panel id="UpcomingMatch3Team2" class="UpcomingMatchTeam2">
              <Image id="UpcomingMatch3Team2Img" class="UpcomingMatchTeam2Img" src="s2r://panorama/images/items/boots_png.vtex"/>
            </Panel>
          </Panel>
          <Label id="UpcomingMatch3Stage" class="UpcomingMatchStage" text="Lower Bracket Round 4 (Bo3)"/>
          <Label id="UpcomingMatch3Status" class="UpcomingMatchStatus" text="Starting in 03:24"/>
          <Label id="UpcomingMatch3WinnerStakes" class="UpcomingMatchWinnerStakes" text="Winner advances to Lower Finals"/>
          <Label id="UpcomingMatch3LoserStakes" class="UpcomingMatchLoserStakes" text="Loser is eliminated, Wins $125,523"/>
        </Panel>
      </Panel>
      <Panel id="TeamsPanel">
        <Label id="TeamsLabel" text="Teams"/>
        <Panel id="TeamsList">
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
          <AnimatedImageStrip class="TeamsListImage" src="s2r://panorama/images/emoticons/dchorse_png.vtex" frametime="100ms" />
        </Panel>
      </Panel>
      <Panel id="CompendiumPanel">
        <Label id="CompendiumLabel" text="Compendium"/>
        <Panel id="CompendiumButtons">
          <Button  class="PopupButton" id="PurchaseButton" onactivate="DOTATournamentBracketShowMatchDetails( 3 )">
            <Label text="Purchase: $4.99" />
          </Button>
        </Panel>
        <Label id="ContributionLabel" text="25% of the proceeds of each sale contributes to the prize pool."/>
      </Panel>
    </Panel>

  </PopupTournamentBracket>
</root>
