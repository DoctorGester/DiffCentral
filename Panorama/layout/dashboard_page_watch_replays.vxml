<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_watch_replays.vcss_c" />
	</styles>

	<DOTAWatchReplaysPage class="DashboardPage">

		<Panel id="Row0" class="RowContainer">
			<Label class="RowHeader" text="#DOTA_WatchReplays_TournamentsHeader" />
			<Panel class="TransportControls">
				<Panel class="TransportControlsDots">
					<Panel id="Row0Dot0" class="ReplayDot"/>
					<Panel id="Row0Dot1" class="ReplayDot"/>
					<Panel id="Row0Dot2" class="ReplayDot"/>
					<Panel id="Row0Dot3" class="ReplayDot"/>
					<Panel id="Row0Dot4" class="ReplayDot"/>
					<Panel id="Row0Dot5" class="ReplayDot"/>
					<Panel id="Row0Dot6"  class="ReplayDot"/>
					<Panel id="Row0Dot7"  class="ReplayDot"/>
					<Panel id="Row0Dot8"  class="ReplayDot"/>
					<Panel id="Row0Dot9"  class="ReplayDot"/>
					<Panel id="Row0Dot10"  class="ReplayDot"/>
				</Panel>
				<Panel class="TransportControlsDots">
					<Panel id="Row0Left" class="ReplayArrow Left" onactivate="ListSegmentViewRetreat(Row0List)"/>
					<Panel id="Row0Right" class="ReplayArrow" onactivate="ListSegmentViewAdvance(Row0List)"/>
				</Panel>
			</Panel>
			<Panel class="RowContent">
				<ListSegmentView id="Row0List" class="RowListSegmentView" items-displayed="4" step-size="4">
				</ListSegmentView>
			</Panel>
			<Panel id="Row0Spinner" class="ReplaysSpinner"><Panel class="Spinner"/></Panel>
		</Panel>

		<Panel id="Row1" class="RowContainer">
			<Label class="RowHeader" text="#DOTA_WatchReplays_FriendsHeader" />
			<Panel class="TransportControls">
				<Panel class="TransportControlsDots">
					<Panel id="Row1Dot0"  class="ReplayDot"/>
					<Panel id="Row1Dot1"  class="ReplayDot"/>
					<Panel id="Row1Dot2"  class="ReplayDot"/>
					<Panel id="Row1Dot3"  class="ReplayDot"/>
					<Panel id="Row1Dot4"  class="ReplayDot"/>
					<Panel id="Row1Dot5"  class="ReplayDot"/>
					<Panel id="Row1Dot6"  class="ReplayDot"/>
					<Panel id="Row1Dot7"  class="ReplayDot"/>
					<Panel id="Row1Dot8"  class="ReplayDot"/>
					<Panel id="Row1Dot9"  class="ReplayDot"/>
					<Panel id="Row1Dot10"  class="ReplayDot"/>
				</Panel>
				<Panel class="TransportControlsDots">
					<Panel id="Row1Left" class="ReplayArrow Left" onactivate="ListSegmentViewRetreat(Row1List)"/>
					<Panel id="Row1Right" class="ReplayArrow" onactivate="ListSegmentViewAdvance(Row1List)"/>
				</Panel>
			</Panel>
			<Panel class="RowContent">
				<ListSegmentView id="Row1List" class="RowListSegmentView" items-displayed="4" step-size="4">
				</ListSegmentView>
			</Panel>
			<Panel id="Row1Spinner" class="ReplaysSpinner"><Panel class="Spinner"/></Panel>
		</Panel>

		<Panel id="Row2" class="RowContainer">
			<Label class="RowHeader" text="#DOTA_WatchReplays_SavedForLater" />
			<Panel class="TransportControls">
				<Panel class="TransportControlsDots">
					<Panel id="Row2Dot0"  class="ReplayDot"/>
					<Panel id="Row2Dot1"  class="ReplayDot"/>
					<Panel id="Row2Dot2"  class="ReplayDot"/>
					<Panel id="Row2Dot3"  class="ReplayDot"/>
					<Panel id="Row2Dot4"  class="ReplayDot"/>
					<Panel id="Row2Dot5"  class="ReplayDot"/>
					<Panel id="Row2Dot6"  class="ReplayDot"/>
					<Panel id="Row2Dot7"  class="ReplayDot"/>
					<Panel id="Row2Dot8"  class="ReplayDot"/>
					<Panel id="Row2Dot9"  class="ReplayDot"/>
					<Panel id="Row2Dot10"  class="ReplayDot"/>
				</Panel>
				<Panel class="TransportControlsDots">
					<Panel id="Row2Left" class="ReplayArrow Left" onactivate="ListSegmentViewRetreat(Row2List)"/>
					<Panel id="Row2Right" class="ReplayArrow" onactivate="ListSegmentViewAdvance(Row2List)"/>
				</Panel>
			</Panel>
			<Panel class="RowContent">
				<ListSegmentView id="Row2List" class="RowListSegmentView" items-displayed="4" step-size="4">
				</ListSegmentView>
			</Panel>
			<Panel id="Row2Spinner" class="ReplaysSpinner"><Panel class="Spinner"/></Panel>
		</Panel>

	</DOTAWatchReplaysPage>
</root>
