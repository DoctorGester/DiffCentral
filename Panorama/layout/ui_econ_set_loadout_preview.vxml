<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/ui_econ_set_loadout_preview.vcss_c" />
	</styles>

	<DOTAUIEconSetLoadoutPreview class="HeroSetFrame" >
		<Panel class="HeroSetDisplay">
			<DOTAUIEconSetPreview id="EconSetPreview" drawbackground="false" />
			<Panel class="LeftRightFlow SetIcons" >
				<Panel class="HeroSetBundle" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_InBundle )" onmouseout="DOTAHideTextTooltip()"  />
				<Panel class="HeroSetPartialSet" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_PartialSet )" onmouseout="DOTAHideTextTooltip()" />
				<Panel class="HeroSetMultiStyle" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_MultipleStyles )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="HeroSetMultiTeam" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_MultipleTeams )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="EmptySocketIcon" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasEmptySocket )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="GemIcon" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasCommonGem )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="RareGemIcon" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasRareGem )" onmouseout="DOTAHideTextTooltip()" />
      </Panel>
			<Panel class="HeroSetInfo">
				<Label class="HeroSetName" text="#DOTA_EconItemSet_Name" />
				<Label class="HeroSetRarityName ItemRarityColor" text="#DOTA_EconItemSet_Rarity" />
			</Panel>
			<Label class="DeveloperOnlyLabel" text="#DOTA_EconItemSet_DeveloperOnly" />
			<Panel id ="HeroSetEquipped" hittest="false"/>
		</Panel>
		<Panel class="HeroSetRarity ItemRarityBackgroundColor" />
		<Panel class="HeroSetPriceFrame" >
			<Panel class="LeftRightFlow">
				<Label class="HeroSetStatus" text="#DOTA_EconItemSet_Status" />
				<Label class="HeroSetSaleAmount" text="#DOTA_EconItemSet_SaleAmount" />
			</Panel>
			<Panel class="PriceContainer">
				<Panel class="TreasureIcon" />
				<Label class="HeroSetPrice" text="#DOTA_EconItemSet_Price" />
			</Panel>
		</Panel>
	</DOTAUIEconSetLoadoutPreview>
	
</root>
